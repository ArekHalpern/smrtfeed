generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DEV_URL")
}

model Paper {
  id        String   @id @default(uuid())
  title     String
  abstract  String
  authors   String[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
